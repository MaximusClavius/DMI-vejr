- sensor:
    name: local weatherstation - DMI
    command: "curl -s 'https://dmigw.govcloud.dk/v2/metObs/collections/observation/items?stationId=06120&period=latest-10-minutes&api-key=********-****-****-****-************'"
    scan_interval: 600
    value_template: '{% if value_json is not none %}OK - {{as_timestamp(value_json["timeStamp"])|timestamp_local}} - Antal: {{value_json["numberReturned"]}}{% else %}Unavailable{% endif %}'
    json_attributes:
      - features
      - timeStamp
